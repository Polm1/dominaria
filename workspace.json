{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "mirari",
  "projects": {
    "mirari": {
      "root": "apps/mirari",
      "sourceRoot": "apps/mirari/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "fetch-versions": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "node tools/scripts/fetch-versions.js apps/mirari"
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/mirari",
            "index": "apps/mirari/src/index.html",
            "main": "apps/mirari/src/main.tsx",
            "polyfills": "apps/mirari/src/polyfills.ts",
            "tsConfig": "apps/mirari/tsconfig.app.json",
            "webpackConfig": "webpack-react-apps.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mirari/src/environments/environment.ts",
                  "with": "apps/mirari/src/environments/environment.prod.ts"
                }
              ],
              "baseHref": "/apps/mirari/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "mirari:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "mirari:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mirari/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mirari/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mirari-e2e": {
      "root": "apps/mirari-e2e",
      "sourceRoot": "apps/mirari-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mirari-e2e/cypress.json",
            "tsConfig": "apps/mirari-e2e/tsconfig.e2e.json",
            "devServerTarget": "mirari:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mirari:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mirari-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "components": {
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components"],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mirari-feature-cards-search": {
      "root": "libs/mirari/feature-cards-search",
      "sourceRoot": "libs/mirari/feature-cards-search/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/mirari/feature-cards-search/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/mirari/feature-cards-search"],
          "options": {
            "jestConfig": "libs/mirari/feature-cards-search/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mirari-state": {
      "root": "libs/mirari/state",
      "sourceRoot": "libs/mirari/state/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/mirari/state/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/mirari/state"],
          "options": {
            "jestConfig": "libs/mirari/state/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api": {
      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api"],
          "options": {
            "jestConfig": "libs/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
